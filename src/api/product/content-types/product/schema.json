{
  "kind": "collectionType",
  "collectionName": "products",
  "info": {
    "singularName": "product",
    "pluralName": "products",
    "displayName": "product"
  },
  "options": {
    "draftAndPublish": false
  },
  "pluginOptions": {},
  "attributes": {
    "title": {
      "type": "string",
      "maxLength": 30,
      "minLength": 1,
      "required": true
    },
    "slug": {
      "type": "uid",
      "targetField": "title"
    },
    "description": {
      "type": "text"
    },
    "size": {
      "type": "enumeration",
      "enum": [
        "small",
        "medium",
        "large"
      ]
    },
    "price": {
      "type": "integer"
    },
    "quantity": {
      "type": "integer"
    }
  }
}
